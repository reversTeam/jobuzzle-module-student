class StudentDataTemplateViewView extends BaseUserDataTemplateViewView {

	_class = 'student';
	_elementType = 'Student';

	display : function () {
		var student = this.state.entity;
		if (!student)
			return (<div>Student not loaded</div>);
		return <div>
			<img src={ 'http:'+ student.getPicture() }/> {student.getFirstname() +' '+ student.getLastname()}
		</div>;
	}

}