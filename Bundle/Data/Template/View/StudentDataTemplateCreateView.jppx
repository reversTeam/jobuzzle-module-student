class StudentDataTemplateCreateView extends BaseUserDataTemplateCreateView {

	_class = 'student';
	_elementType = 'Student';

	public display : function () {
		var createForm = this.getService().getFieldset().getCreateForm();
		return <div>{ this.getHelper('CreateForm', { form : createForm, entity : this.state.entity }) }</div>;
	}

}